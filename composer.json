{
  "name": "nitsan/ns-statcounter",
  "type": "typo3-cms-extension",
  "description": "StatCounter TYPO3 Extension brings you all the powerful StatCounter features to your TYPO3 site.",
  "homepage": "https://extensions.typo3.org/extension/ns_statcounter/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "counter",
    "stat",
    "ns_statcounter",
    "nitsan"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_statcounter/issues"
  },
  "require": {
    "typo3/cms-core": ">=6.2.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsStatcounter\\": "Classes/"
    }
  },
  "replace": {
      "ns_statcounter": "self.version",
      "nitsan/ns-statcounter": "self.version"
  },
  "extra": {
      "typo3/cms": {
          "extension-key": "ns_statcounter"
      }
  }
}
