{
  "name": "nitsan/ns-statcounter",
  "type": "typo3-cms-extension",
  "description": "Statcounter extension brings you all the powerful StatCounter.com (free real time visitor stats) features to your TYPO3 site. Live-Demo: https://demo.t3terminal.com/t3t-extensions/ You can download PRO version for more-features & free-support at https://t3terminal.com/typo3-statcounter-plugin-free/",
  "homepage": "https://extensions.typo3.org/extension/ns_statcounter/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "counter",
    "stat",
    "ns_statcounter",
    "nitsan"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_statcounter/issues"
  },
  "require": {
    "typo3/cms-core": ">=6.0.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsStatcounter\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/ns-statcounter": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_statcounter"
    }
  }
}
